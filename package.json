{
  "name": "prop-model",
  "version": "0.1.0",
  "description": "A model for properties including change events and derived properties",
  "main": "dist/node/index.js",
  "keywords": [
    "mvc",
    "model",
    "properties",
    "change-event",
    "change-listener",
    "onchange"
  ],
  "files": [
    "index.js",
    "lib/prop-model.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mearns/prop-model.git"
  },
  "author": "Brian Mearns <mearns.b@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mearns/prop-model/issues"
  },
  "homepage": "https://github.com/mearns/prop-model/",
  "engines": {
    "node": "^8.0.0 ^9.0.0 ^10.0.0"
  },
  "scripts": {
    "clean": "rimraf coverage/ .nyc_output/ out/ dist/",
    "check:lint:readme:markdown": "remark --no-stdout README.md",
    "check:lint:readme": "npm-run-all --parallel check:lint:readme:*",
    "check:lint:js:root": "standard ./*.js | snazzy",
    "check:lint:js:lib": "standard lib/**/*.js | snazzy",
    "check:lint:js:test": "standard test/**/*.js | snazzy",
    "check:lint:js:npmpackagejsonlint-config": "standard ./npmpackagejsonlint.config.js | snazzy",
    "check:lint:js": "npm-run-all --parallel check:lint:js:*",
    "check:lint:package-json": "npmPkgJsonLint .",
    "check:lint": "npm-run-all --parallel check:lint:*",
    "check": "npm-run-all --parallel check:*",
    "test-only": "mocha --recursive ./test",
    "test": "nyc mocha --recursive ./test",
    "verify": "npm-run-all check test",
    "compile": "echo \"Nothing to build for this project, runs from src.\"",
    "docs": "jsdoc --recurse --destination out/jsdocs --package package.json --configure jsdoc.conf.json index.js",
    "build": "npm-run-all clean verify compile"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "index.js",
      "lib/**"
    ]
  },
  "devDependencies": {
    "chai": "4.2.0",
    "jsdoc": "3.5.5",
    "jsdoc-to-markdown": "4.0.1",
    "mkdirp": "0.5.1",
    "mocha": "5.2.0",
    "npm-package-json-lint": "3.3.0",
    "npm-run-all": "4.1.3",
    "nyc": "13.1.0",
    "remark-cli": "5.0.0",
    "rimraf": "2.6.2",
    "sinon": "7.1.1",
    "sinon-chai": "3.3.0",
    "snazzy": "7.1.1",
    "standard": "12.0.1"
  }
}
